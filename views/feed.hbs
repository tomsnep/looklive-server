
       {{#each items}}
        <div class="feed-item" data-uuid="{{id}}">
            <img src="{{ image.url }}">
            <h2><img class="avatar" src="{{ subject.avatar.url }}"> {{ subject.name }}</h2>
            <h1><a data-url="/api/appearance/{{id}}" href="appearance/{{id}}">{{title}}</a></h1>
        </div>
    {{/each}}